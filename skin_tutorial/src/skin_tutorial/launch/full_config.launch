<?xml version="1.0"?>
<launch>
	<!-- This variable indicates the path where the skin config file (to be created) will be saved /-->  
	<arg name="DEFAULT_PATH" default="$(find skin_tutorial)/launch/configs"/>

	<node name="$(anon full_config_test)" 
		pkg="tum_ics_skin_full_config" 
		type="full_config" 
		args="$(arg DEFAULT_PATH)"
		output="screen">
		<rosparam file="$(find skin_tutorial)/launch/configs/skin_driver_ns_list.yaml" command="load" />
		<rosparam file="$(find skin_tutorial)/launch/configs/base_frame_list.yaml" command="load" />
	</node>
</launch>
